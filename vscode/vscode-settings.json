{
  /**
   Editor stuff
  */
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.bracketPairColorization.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.copyWithSyntaxHighlighting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.enablePreview": false,
  "window.newWindowDimensions": "inherit",
  "workbench.colorTheme": "GitHub Dark",
  "files.trimTrailingWhitespace": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 17,
  "editor.suggest.preview": true,

  /**
    Less "noise"
  */
  "workbench.tips.enabled": false,
  "workbench.startupEditor": "none",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",

  /**
    Prettier
  */
  "prettier.trailingComma": "all",
  "prettier.singleQuote": false,
  "prettier.printWidth": 100,
  "prettier.tabWidth": 2,
  "prettier.semi": true,

  /**
   Discord presence
  */
  "discord.largeImage": "‚ú® Editing a {LANG} file",
  "discord.largeImageIdling": "‚è± Idling",
  "editor.suggest.showStatusBar": true,
  "discord.detailsEditing": "üíª Editing {file_name} in {workspace}",
  "discord.lowerDetailsEditing": "üìÑ line: {current_line}/{total_lines} - Branch: {git_branch} - {file_size}",

  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "scm.providerCountBadge": "auto",
  "files.associations": {
    ".env": "dotenv"
  },
  "cSpell.userWords": [
    "Hoverable",
    "URL's",
    "cadv",
    "caspertheghost",
    "citizenfx",
    "nocheck",
    "prefetch",
    "zyndicate"
  ],

  "editor.linkedEditing": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "extensions.ignoreRecommendations": true,
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",

  "files.eol": "\n",
  "remote.WSL.server.connectThroughLocalhost": true,

  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "redhat.telemetry.enabled": false,
  "material-icon-theme.showWelcomeMessage": false,
  "window.dialogStyle": "custom"
}
