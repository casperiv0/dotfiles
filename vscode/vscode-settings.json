{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.bracketPairColorization.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.copyWithSyntaxHighlighting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.enablePreview": false,
  "workbench.colorTheme": "GitHub Dark",
  "files.trimTrailingWhitespace": true,
  "editor.fontFamily": "Fira Code",
  "editor.suggest.preview": true,
  "html.completion.attributeDefaultValue": "doublequotes",
  "editor.tabSize": 2,
  "editor.lightbulb.enabled": false,
  "editor.detectIndentation": true,
  "editor.fontSize": 17,
  "editor.hover.enabled": true,
  "workbench.tips.enabled": false,
  "workbench.startupEditor": "none",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "debug.javascript.codelens.npmScripts": "never",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": false,
  "prettier.trailingComma": "all",
  "prettier.singleQuote": false,
  "prettier.printWidth": 100,
  "prettier.tabWidth": 2,
  "prettier.semi": true,
  "discord.largeImage": "‚ú® Editing a {LANG} file",
  "discord.largeImageIdling": "‚è± Idling",
  "editor.suggest.showStatusBar": true,
  "discord.detailsEditing": "üíª Editing {file_name} in {workspace}",
  "discord.lowerDetailsEditing": "üìÑ line: {current_line}/{total_lines} - Branch: {git_branch} - {file_size}",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "scm.providerCountBadge": "auto",
  "files.associations": {
    ".env": "dotenv",
    "*.md": "mdx"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true
  },
  "cSpell.userWords": [
    "Bleeter",
    "cadv",
    "Callsign",
    "caspertheghost",
    "citizenfx",
    "Dompurify",
    "driverslicense",
    "ethnicitie",
    "Formik",
    "Hoverable",
    "nocheck",
    "prefetch",
    "URL's",
    "zyndicate"
  ],
  "editor.linkedEditing": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "extensions.ignoreRecommendations": true,
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",
  "files.eol": "\n",
  "remote.WSL.server.connectThroughLocalhost": true,
  "material-icon-theme.showWelcomeMessage": false,
  "window.dialogStyle": "custom",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.language": "en,en-GB,en-US",
  "material-icon-theme.files.associations": {
    "*.docker-compose.yml": "docker"
  },
  "material-icon-theme.folders.associations": {
    "workflows": "ci",
    "ISSUE_TEMPLATE": "template",
    "state": "context",
  },
  "js/ts.implicitProjectConfig.strictNullChecks": true
}
