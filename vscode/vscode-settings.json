{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.bracketPairColorization.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.copyWithSyntaxHighlighting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.enablePreview": false,
  "window.newWindowDimensions": "inherit",
  "workbench.colorTheme": "GitHub Dark",
  "files.trimTrailingWhitespace": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 17,
  "editor.tabSize": 2,
  "editor.suggest.preview": true,
  "editor.lightbulb.enabled": false,
  "editor.detectIndentation": true,
  "workbench.tips.enabled": false,
  "workbench.startupEditor": "none",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.suggest.localityBonus": true,
  "prettier.trailingComma": "all",
  "prettier.singleQuote": false,
  "prettier.printWidth": 100,
  "prettier.tabWidth": 2,
  "prettier.semi": true,
  "discord.largeImage": "‚ú® Editing a {LANG} file",
  "discord.largeImageIdling": "‚è± Idling",
  "editor.suggest.showStatusBar": true,
  "discord.detailsEditing": "üíª Editing {file_name} in {workspace}",
  "discord.lowerDetailsEditing": "üìÑ line: {current_line}/{total_lines} - Branch: {git_branch} - {file_size}",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[lua]": {
    "editor.defaultFormatter": "Koihik.vscode-lua-format"
  },
  "scm.providerCountBadge": "auto",
  "files.associations": {
    ".env": "dotenv",
    "*.md": "mdx"
  },
  "cSpell.userWords": [
    "Hoverable",
    "URL's",
    "cadv",
    "caspertheghost",
    "citizenfx",
    "nocheck",
    "prefetch",
    "zyndicate"
  ],
  "editor.linkedEditing": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "extensions.ignoreRecommendations": true,
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",
  "files.eol": "\n",
  "remote.WSL2.connectionMethod": "localhost",
  "material-icon-theme.showWelcomeMessage": false,
  "window.dialogStyle": "custom",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.language": "en,en-GB,en-US",
  "gitlens.codeLens.includeSingleLineSymbols": true,
  "gitlens.defaultDateFormat": "MMMM D, YYYY hh:mm",
  "gitlens.defaultTimeFormat": "hh:mm",
  "gitlens.defaultDateShortFormat": null,
  "redhat.telemetry.enabled": false,
  "svelte.enable-ts-plugin": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "workbench.experimental.layoutControl.type": "both"
}